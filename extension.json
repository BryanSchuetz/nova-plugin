{
  "identifier": "emmetio.Emmet",
  "name": "Emmet",
  "vendor": "Emmet",
  "description": "Emmet â€” the essential toolkit for web-developers",
  "version": "1.0",
  "categories": [],
  "main": "main.js",
  "activationEvents": [
    "onLanguage:html"
  ],
  "entitlements": {
    "filesystem": "readwrite",
    "requests": true
  },
  "commands": {
    "editor": [{
      "title": "Expand Abbreviation",
      "command": "emmet.expand-abbreviation"
    }, {
      "title": "Balance",
      "command": "emmet.balance",
      "shortcut": "ctrl-d"
    }, {
      "title": "Balance Inward",
      "command": "emmet.balance-inward",
      "shortcut": "ctrl-shift-d"
    }, {
      "title": "Comment Section",
      "command": "emmet.comment",
      "shortcut": "ctrl-c"
    }, {
      "title": "Convert to data:URL",
      "command": "emmet.convert-data-url",
      "shortcut": "ctrl-i"
    }]
  }
}
