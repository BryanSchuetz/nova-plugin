{
  "compilerOptions": {
    "target": "ES6",
    "sourceMap": true,
    "noUnusedLocals": true,
    "alwaysStrict": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "declaration": false,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "lib": ["ES6"],
    "types": [],
    "module": "commonjs",
    "newLine": "LF",
    "outDir": "./Emmet.novaextension"
  },
  "include": ["src/*.d.ts", "src/**/*.ts"],
  "exclude": ["./node_modules/**"]
}
